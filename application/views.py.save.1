        ed = (6.108 * np.exp((17.27 * J11) / (J11 + 237.3)) * F11 +
              6.108 * np.exp((17.27 * I11) / (I11 + 237.3)) * G11) / 200

    Œî = 4098.171 * ea_Tmoy / (ea_Tmoy + 237.3) ** 2
    dr = 1 + 0.033 * np.cos(2 * np.pi * D11 / 365)
    Œ¥ = 0.4093 * np.sin(2 * np.pi * (284 + D11) / 365)
    œâs = np.arccos(-np.tan(np.radians(B6)) * np.tan(Œ¥))
    Rsmm = K11 / Œª
    Ra = (24 / np.pi) * 1367 * dr * (œâs * np.sin(np.radians(B6)) * np.sin(Œ¥) +
                                     np.cos(np.radians(B6)) * np.cos(Œ¥) * np.sin(œâs))
    Ramm = Ra / Œª
    Rso = (0.75 + 2 * (10 ** -5) * A6) * Ramm

    if pd.isna(F11) or pd.isna(I11):
        Rn = 0.77 * Rsmm - (1.35 * (Rsmm / Rso) - 0.35) * (0.34 - 0.14 * np.sqrt(ed)) * (1360.8 * (10 ** -9) / Œª) * (H11 + 273.16) ** 4
    else:
        Rn = (0.77 * Rsmm - (1.35 * (Rsmm / Rso) - 0.35) * (0.34 - 0.14 * np.sqrt(ed)) * (1360.8 * (10 ** -9) / Œª) *
              ((I11 + 273.16) ** 4 + (J11 + 273.16) ** 4) / 2)

    ETrad = (Œî * Rn) / (Œî + Œ≥_prime)

    if np.isnan(H11):
        ETaero = (Œ≥ * (90 / ((I11 + J11) / 2 + 273.16)) * U2 * (ea_Tmoy - ed)) / (Œî + Œ≥_prime)
    else:
        ETaero = (Œ≥ * (90 / (H11 + 273.16)) * U2 * (ea_Tmoy - ed)) / (Œî + Œ≥_prime)

    ETo = ETrad + ETaero

    # R√©sultats
    print(f"ETo: {ETo} mm/jour")

    # Enregistrement dans la base de donn√©es
    ET0o.objects.create(
        value=round(ETo,2),
        WSavg=L11,
        Tmax=I11,
        Tmin=J11,
        Tavg=H11,
        Hmax=F11,
        Hmin=G11,
        Raym=round(K11, 2),
        U2=U2,
        Delta=D11
    )


    I11 = data['temp_max']  # Temp√©rature max (¬∞C)
    J11 = data['temp_min']  # Temp√©rature min (¬∞C)
    K11 = data['radiation_sum']  # Radiation solaire (MJ/m¬≤)
    L11 = data['wind_speed_avg']  # Vitesse moyenne du vent (m/s)

    # Remplacement des None par une valeur par d√©faut (ex: 0 ou une moyenne raisonnable)
    # I11 = I11 if I11 is not None else 0
    # J11 = J11 if J11 is not None else 0
    # H11 = H11 if H11 is not None else (I11 + J11) / 2
    # F11 = F11 if F11 is not None else 0
    # G11 = G11 if G11 is not None else 0
    # E6 = E6 if E6 is not None else 1013  # Pression atmosph√©rique standard

    # Calculs
    P = 1013 * ((293 - 0.0065 * A6) / 293) ** 5.256
    Œª = 694.5 * (1 - 0.000946 * H11)
    Œ≥ = 0.2805555 * E6 / (0.622 * Œª)
    U2 = 4.868 * L11 / np.log(67.75 * C6 - 5.42)
    Œ≥_prime = Œ≥ * (1 + 0.34 * U2)

    if np.isnan(H11):
        ea_Tmoy = 6.108 * np.exp((17.27 * (I11 + J11) / 2) / ((I11 + J11) / 2 + 237.3))
    else:
        ea_Tmoy = 6.108 * np.exp((17.27 * H11) / (H11 + 237.3))

    if pd.isna(F11) or pd.isna(I11):
        ed = ea_Tmoy * E6 / 100
    else:
        ed = (6.108 * np.exp((17.27 * J11) / (J11 + 237.3)) * F11 +
              6.108 * np.exp((17.27 * I11) / (I11 + 237.3)) * G11) / 200

    Œî = 4098.171 * ea_Tmoy / (ea_Tmoy + 237.3) ** 2
    dr = 1 + 0.033 * np.cos(2 * np.pi * D11 / 365)
    Œ¥ = 0.4093 * np.sin(2 * np.pi * (284 + D11) / 365)
    œâs = np.arccos(-np.tan(np.radians(B6)) * np.tan(Œ¥))
    Rsmm = K11 / Œª
    Ra = (24 / np.pi) * 1367 * dr * (œâs * np.sin(np.radians(B6)) * np.sin(Œ¥) +
                                     np.cos(np.radians(B6)) * np.cos(Œ¥) * np.sin(œâs))
    Ramm = Ra / Œª
    Rso = (0.75 + 2 * (10 ** -5) * A6) * Ramm

    if pd.isna(F11) or pd.isna(I11):
        Rn = 0.77 * Rsmm - (1.35 * (Rsmm / Rso) - 0.35) * (0.34 - 0.14 * np.sqrt(ed)) * (1360.8 * (10 ** -9) / Œª) * (H11 + 273.16) ** 4
    else:
        Rn = (0.77 * Rsmm - (1.35 * (Rsmm / Rso) - 0.35) * (0.34 - 0.14 * np.sqrt(ed)) * (1360.8 * (10 ** -9) / Œª) *
              ((I11 + 273.16) ** 4 + (J11 + 273.16) ** 4) / 2)

    ETrad = (Œî * Rn) / (Œî + Œ≥_prime)

    if np.isnan(H11):
        ETaero = (Œ≥ * (90 / ((I11 + J11) / 2 + 273.16)) * U2 * (ea_Tmoy - ed)) / (Œî + Œ≥_prime)
    else:
        ETaero = (Œ≥ * (90 / (H11 + 273.16)) * U2 * (ea_Tmoy - ed)) / (Œî + Œ≥_prime)

    ETo = ETrad + ETaero

